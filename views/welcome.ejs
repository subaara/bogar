<% layout('layout') -%>


<h1> This is welcome page </h1>

<a href="#" id="logout"> Logout </a>
<script> 
    var app = angular.module('loginApp', []);

    $('#logout').click(function() {
        $.ajax({
            url: '/logout',
            type: 'get',
            data: {},
            success: function() {
                window.location = '/';
            },
            failure: function() {
                alert('failure')
            }
        })
    })
</script>